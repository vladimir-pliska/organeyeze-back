type Query {
    getContainer(id: ID!): Container!
    getContainers: [Container!]!
}

type Mutation {
    upsertContainer(input: UpsertContainerInput!): Container!
}

type Container {
    id: ID!
    name: String!
    description: String
    content: [String!]!
}

input UpsertContainerInput {
    id: ID
    name: String!
    description: String
    content: [String]!
}
